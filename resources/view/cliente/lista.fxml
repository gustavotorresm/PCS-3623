<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.BorderPane?>

<ScrollPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.usp.poli.magnodb.controller.EntregaListaController">
    <content>
        <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
            <center>
                <TableView fx:id="tabela" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                    <columns>
                        <TableColumn prefWidth="60.0" text="id">
                            <cellValueFactory><PropertyValueFactory property="id" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="74.0" text="Vendas">
                            <cellValueFactory><PropertyValueFactory property="venda" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="119.0" text="Data de Pagamento">
                            <cellValueFactory><PropertyValueFactory property="dataPagamento" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn minWidth="2.0" prefWidth="113.0" text="Data de Despache">
                            <cellValueFactory><PropertyValueFactory property="dataDespache" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn minWidth="7.0" prefWidth="121.0" text="Data de Recepcao">
                            <cellValueFactory><PropertyValueFactory property="dataRecepcao" />
                            </cellValueFactory>
                        </TableColumn>
                        <!--<TableColumn prefWidth="171.0" text="descrição" />
                        <TableColumn prefWidth="75.0" text="preço" />
                        <TableColumn prefWidth="75.0" text="criação" />-->
                    </columns>
                </TableView>
            </center>
        </BorderPane>
    </content>
</ScrollPane>
